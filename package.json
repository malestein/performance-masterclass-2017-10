{
  "name": "performance-masterclass-2017-10",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prebuild": "rimraf cache/ && node scripts/copy.js",
    "build": "run-s build:*",
    "build:css": "node scripts/styles.js",
    "build:js": "node scripts/scripts.js",
    "postbuild": "run-s revision compress",
    "compress": "node scripts/compress.js",
    "dev": "run-p start watch",
    "prestart": "npm run build",
    "proxy": "npm-run-all --parallel proxy:*",
    "proxy:local": "browser-sync start --proxy 'localhost:6521' --port 6524",
    "proxy:public": "ngrok http 6521",
    "revision": "node scripts/revision-hash.js",
    "start": "nodemon --expose-http2 server.js",
    "watch": "chokidar 'src/**' --command 'npm run build'"
  },
  "author": "De Voorhoede",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "1.2.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-brotli": "1.2.0",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.2",
    "gulp-filter": "5.0.0",
    "gulp-gzip": "1.4.0",
    "gulp-rev": "7.1.2",
    "gulp-rev-css-url": "0.1.0",
    "gulp-rev-replace": "0.4.3",
    "gulp-uglify": "3.0.0",
    "ncp": "2.0.0",
    "ngrok": "2.2.22",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "cheerio": "1.0.0-rc.2",
    "express": "5.0.0-alpha.6",
    "helmet": "3.8.2",
    "html-minifier": "3.5.5",
    "mime": "2.0.3",
    "npm-run-all": "4.1.1",
    "nunjucks": "3.0.0",
    "request": "2.83.0",
    "server-timings": "1.0.4",
    "shrink-ray": "0.1.3",
    "spdy": "3.4.7"
  }
}
